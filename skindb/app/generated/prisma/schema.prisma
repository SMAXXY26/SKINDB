generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cases {
  id   String @id(map: "Case_pkey")
  name String
}

model skin {
  id     String  @id(map: "Skin_pkey")
  name   String
  rarity String
  weapon String
  caseId String?
}
